{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "title": "EssexMums Theme",
    "description": "EssexMums theme",
    "settings": {
        "appearanceTools": true,
        "background": {},
        "border": {},
        "color": {
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "palette": [
                {
                    "name": "Pink Hero",
                    "slug": "pink-hero",
                    "color": "#dd74e8"
                },
                {
                    "name": "Pink Pale",
                    "slug": "pink-pale",
                    "color": "#f9c1ff"
                },
                {
                    "name": "Teal Hero",
                    "slug": "teal-hero",
                    "color": "#40cdd2"
                },
                {
                    "name": "Teal Pale",
                    "slug": "teal-pale",
                    "color": "#a8e7e9"
                },
                {
                    "name": "True Black",
                    "slug": "true-black",
                    "color": "#000000"
                },
                {
                    "name": "Half Black",
                    "slug": "half-black",
                    "color": "#4E555E"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                }
            ]
        },
        "custom": {
            "gap": {
                "row": "1.5rem",
                "inline": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
                "block": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
                "button": "0.5rem"
            },
            "padding": {
                "inline": "clamp(1rem, -1.1429rem + 5.7143vw, 4rem)",
                "block": "clamp(1.5rem, 8.75vw + -1.781rem, 5rem)"
            },
            "margin": {
                "inline": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
                "block": "clamp(1.5rem, 8.75vw + -1.781rem, 5rem)"
            },
            "transition": "all 0.3s cubic-bezier(0.12, 0.72, 0.33, 1)"
        },
        "dimensions": {},
        "layout": {
            "contentSize": "840px",
            "wideSize": "1200px"
        },
        "lightbox": {},
        "position": {},
        "shadow": {},
        "spacing": {
            "customSpacingSize": true,
            "defaultSpacingSizes": false,
            "blockGap": true,
            "spacingSizes": [
                {
                    "size": " clamp(0.25rem, 0.625vw + 0.016rem, 0.5rem)",
                    "slug": "08",
                    "name": "8px"
                },
                {
                    "size": " clamp(0.5rem, 1.25vw + 0.031rem, 1rem)",
                    "slug": "16",
                    "name": "16px"
                },
                {
                    "size": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
                    "slug": "24",
                    "name": "24px"
                },
                {
                    "size": "clamp(1.5rem, 1.25vw + 1.031rem, 2rem)",
                    "slug": "32",
                    "name": "32px"
                },
                {
                    "size": "clamp(2.5rem, 1.25vw + 2.031rem, 3rem)",
                    "slug": "48",
                    "name": "48px"
                },
                {
                    "size": "clamp(3rem, 1.875vw + 2.297rem, 3.75rem)",
                    "slug": "60",
                    "name": "60px"
                },
                {
                    "size": "clamp(4.5rem, 1.875vw + 3.797rem, 5.25rem)",
                    "slug": "80",
                    "name": "80px"
                }
            ],
            "units": ["%", "px", "rem", "vh", "vw"]
        },
        "typography": {
            "fluid": true,
            "defaultFontSizes": false,
            "fontFamilies": [
                {
                    "name": "Roboto",
                    "slug": "roboto",
                    "fontFamily": "Roboto, sans-serif",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/fonts/roboto/roboto-variable.ttf"
                            ],
                            "fontWeight": "100 900",
                            "fontStyle": "normal",
                            "fontFamily": "Roboto"
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "fluid": {
                        "min": "0.6875rem",
                        "max": "0.75rem"
                    },
                    "name": "Body Small",
                    "size": "0.75rem",
                    "slug": "body-small"
                },
                {
                    "fluid": {
                        "min": "0.75rem",
                        "max": "0.875rem"
                    },
                    "name": "Body Medium",
                    "size": "0.875rem",
                    "slug": "body-medium"
                },
                {
                    "fluid": {
                        "min": "0.875rem",
                        "max": "1rem"
                    },
                    "name": "Body Large",
                    "size": "1rem",
                    "slug": "body-large"
                },
                {
                    "fluid": {
                        "min": "1rem",
                        "max": "1.5rem"
                    },
                    "name": "Headline Small",
                    "size": "1.5rem",
                    "slug": "headline-small"
                },
                {
                    "fluid": {
                        "min": "1.125rem",
                        "max": "1.75rem"
                    },
                    "name": "Headline Medium",
                    "size": "1.75rem",
                    "slug": "headline-medium"
                },
                {
                    "fluid": {
                        "min": "1.5rem",
                        "max": "2.125rem"
                    },
                    "name": "Headline Large",
                    "size": "2.125rem",
                    "slug": "headline-large"
                },
                {
                    "fluid": {
                        "min": "1.75rem",
                        "max": "2.625rem"
                    },
                    "name": "Display Small",
                    "size": "2.625rem",
                    "slug": "display-small"
                },
                {
                    "fluid": {
                        "min": "2.25rem",
                        "max": "3.125rem"
                    },
                    "name": "Display Medium",
                    "size": "3.125rem",
                    "slug": "display-medium"
                },
                {
                    "fluid": {
                        "min": "2.5rem",
                        "max": "3.75rem"
                    },
                    "name": "Display Large",
                    "size": "3.75rem",
                    "slug": "display-large"
                }
            ],
            "writingMode": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "color": {
            "background": "var:preset|color|white",
            "text": "var:preset|color|secondary"
        },
        "typography": {
            "fontFamily": "var:preset|font-family|roboto",
            "fontSize": "var:preset|font-size|body-large",
            "lineHeight": "1.5",
            "fontStyle": "normal",
            "fontWeight": "400"
        },
        "elements": {
            "button": {
                "typography": {
                    "fontSize": "var:preset|font-size|body-large",
                    "lineHeight": "1.5"
                }
            },
            "heading": {
                "color": {
                    "text": "var:preset|color|secondary"
                }
            },
            "link": {
                "color": {
                    "text": "var:preset|color|secondary"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                },
                "typography": {
                    "textDecoration": "none"
                }
            },
            "h1": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|display-large",
                    "lineHeight": "1.1",
                    "fontWeight": "500"
                }
            },
            "h2": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|display-medium",
                    "lineHeight": "1.1",
                    "fontWeight": "500"
                }
            },
            "h3": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|display-small",
                    "lineHeight": "1.2",
                    "fontWeight": "500"
                }
            },
            "h4": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|headline-large",
                    "lineHeight": "1.2",
                    "fontWeight": "500"
                }
            },
            "h5": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|headline-medium",
                    "lineHeight": "1.3",
                    "fontWeight": "500"
                }
            },
            "h6": {
                "typography": {
                    "fontFamily": "var:preset|font-family|roboto",
                    "fontSize": "var:preset|font-size|headline-small",
                    "lineHeight": "1.3",
                    "fontWeight": "500"
                }
            }
        },
        "blocks": {
            "core/button": {
                "typography": {
                    "fontSize": "var:preset|font-size|body-medium",
                    "fontWeight": "400",
                    "lineHeight": "1.5",
                    "letterSpacing": "0.03em"
                }
            },
            "core/buttons": {
                "typography": {
                    "fontSize": "var:preset|font-size|body-medium",
                    "fontWeight": "400",
                    "lineHeight": "1.5",
                    "letterSpacing": "0.03em"
                }
            },

            "core/group": {
                "spacing": {
                    "margin": {
                        "bottom": "var(--wp--custom--margin--block)",
                        "top": "0"
                    }
                }
            },
            "core/post-content": {
                "spacing": {
                    "margin": {
                        "bottom": "0",
                        "top": "0"
                    }
                }
            },
            "core/separator": {
                "border": {
                    "color": "var:preset|color|tertiary",
                    "style": "solid",
                    "width": "1px"
                },
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "0"
                    }
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--24)",
            "padding": {
                "right": "var(--wp--custom--padding--inline)",
                "left": "var(--wp--custom--padding--inline)"
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ]
}
